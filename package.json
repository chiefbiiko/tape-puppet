{
  "name": "tape-puppet",
  "version": "0.2.1",
  "description": "duplex stream that runs browserified tape tests with puppeteer",
  "main": "index.js",
  "bin": {
    "tape-puppet": "./cli.js"
  },
  "scripts": {
    "test": "browserify ./test.js | node ./cli.js",
    "cover": "browserify ./test.js --debug -t browserify-istanbul | node ./cli.js --cover && nyc report",
    "view-cover": "opn ./coverage/lcov-report/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chiefbiiko/tape-puppet.git"
  },
  "keywords": [
    "tape",
    "tap",
    "browser",
    "puppeteer",
    "test",
    "runner",
    "headless",
    "chromium"
  ],
  "author": "Noah Anabiik Schwarz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chiefbiiko/tape-puppet/issues"
  },
  "homepage": "https://github.com/chiefbiiko/tape-puppet#readme",
  "dependencies": {
    "minimist": "^1.2.0",
    "pump": "^3.0.0",
    "puppeteer": "^2.0.0",
    "tap-finished": "0.0.1"
  },
  "devDependencies": {
    "browserify": "16.5.1",
    "browserify-istanbul": "3.0.1",
    "opn": "1.0.1",
    "nyc": "15.0.1",
    "tape": "^4.9.1"
  }
}
